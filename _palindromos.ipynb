    "def palindromo(n: int) -> bool:\n",
    "    \"\"\"Identifica se um numero informado é palíndromo.\n",
    "    \"\"\"\n",
    "    \n",
    "    numTxt = list(str(n))\n",
    "\n",
    "    met = len(numTxt) // 2\n",
    "\n",
    "    for pos in range(met):\n",
    "        if numTxt[pos] != numTxt[-pos-1]:\n",
    "            return False\n",
    "        \n",
    "    return True"
   ]
  }
